Proceso circulo
	definir diametro, area, radio Como Real;
	diametro<-0; radio <-0; area<-0;
	escribir "escribe el radio de la circuferencia";
	leer radio;
	diametro<-radio*2;
	area<-PI*radio^2;
	escribir "el radio de definido es ", radio, " el diametro de la circuferencia es: ", diametro, " el area de la circuferencia es ", area, ".";
	
FinProceso
