Proceso rectangulo
	definir area, base, altura Como Entero;
	area<-0; base<-0; altura<-0;
	escribir "digite la base del rectangulo:";
	leer base;
	escribir 'digite la altura del rectangulo";
	leer altura;
	area<-base*altura;
	escribir "el altura del rectangulo es:",altura,"el base rectangulo es:", base, "el area rectangulo es:", area, ".";
FinProceso
